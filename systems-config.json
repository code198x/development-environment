      "assembler_name": "cc65",
      "base_image": "6502-base",
      "architecture": "6502",
      "test_command": "test.asm -o test.o",
      "test_output": "test.o",
      "verify_command": "test.asm -o verify.o",
      "verify_output": "verify.o",
      "file_extension": ".nes",
      "description": "6502 assembly for the console that saved gaming"
    },
    {
